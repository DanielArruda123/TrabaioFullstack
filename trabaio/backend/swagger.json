{
  "openapi": "3.0.0",
  "info": {
    "title": "API do Sistema de Petshop",
    "version": "1.0.0",
    "description": "API para gerenciamento de petshop com autenticação JWT"
  },
  "servers": [
    {
      "url": "http://localhost:4000",
      "description": "Servidor de Desenvolvimento"
    }
  ],
  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      },
      "AdminAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  },
  "paths": {
    "/": {
      "get": {
        "summary": "Bem-vindo a API",
        "tags": ["Geral"],
        "responses": {
          "200": {
            "description": "Mensagem de boas-vindas",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {"type": "string"}
                  }
                }
              }
            }
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "summary": "Login de usuário",
        "tags": ["Autenticação"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {"type": "string"},
                  "password": {"type": "string"}
                },
                "required": ["username", "password"]
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login bem sucedido",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {"type": "string"},
                    "token": {"type": "string"}
                  }
                }
              }
            }
          },
          "401": {"description": "Credenciais inválidas"},
          "404": {"description": "Usuário não encontrado"},
          "500": {"description": "Erro interno do servidor"}
        }
      }
    },
    "/auth/admin": {
      "get": {
        "summary": "Exibir formulário de login ADM",
        "tags": ["Autenticação - ADM"],
        "responses": {
          "200": {"description": "Formulário de login ADM"},
          "302": {"description": "Redireciona para links se já logado"}
        }
      },
      "post": {
        "summary": "Processar login ADM",
        "tags": ["Autenticação - ADM"],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "username": {"type": "string"},
                  "password": {"type": "string"}
                },
                "required": ["username", "password"]
              }
            }
          }
        },
        "responses": {
          "200": {"description": "Login ADM bem sucedido (redireciona)"},
          "400": {"description": "Requisição inválida"},
          "401": {"description": "Senha incorreta"},
          "404": {"description": "Usuário não encontrado"},
          "500": {"description": "Erro interno do servidor"}
        }
      }
    },
    "/auth/admin/api-links": {
      "get": {
        "summary": "Página de links da API para ADM",
        "tags": ["Autenticação - ADM"],
        "security": [
          {"AdminAuth": []}
        ],
        "responses": {
          "200": {"description": "Links da API ADM"},
          "302": {"description": "Redireciona para login ADM se não autenticado"}
        }
      }
    },
    "/auth/admin/logout": {
      "get": {
        "summary": "Logout ADM",
        "tags": ["Autenticação - ADM"],
        "responses": {
          "302": {"description": "Redireciona para login ADM"}
        }
      }
    },
    "/users": {
      "get": {
        "summary": "Listar todos os usuários",
        "tags": ["Usuários"],
        "security": [
          {
            "AdminAuth": []
          }
        ],
